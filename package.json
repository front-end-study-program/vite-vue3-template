{
  "name": "vite-vue3-template",
  "version": "0.0.1",
  "description": "template for vite vue3",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx,.vue --quiet ./"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,tsx,ts,vue}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "pinia": "^2.0.14",
    "vue": "^3.2.25",
    "vue-router": "^4.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@study-sea/eslint-config-vue": "^0.0.4",
    "@study-sea/prettier-config": "^0.0.1",
    "@study-sea/stylelint-config-recommended": "^0.0.2",
    "@vitejs/plugin-vue": "^2.3.3",
    "eslint": "^8.19.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "stylelint": "^14.9.1",
    "typescript": "^4.5.4",
    "unocss": "^0.44.7",
    "vite": "^2.9.9",
    "vite-plugin-eslint": "^1.7.0",
    "vite-plugin-pages": "^0.25.0",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vue-tsc": "^0.34.7"
  }
}